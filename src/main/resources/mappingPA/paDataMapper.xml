  <select id="allComp" resultType="com.yiban.erp.entitiesPA.compInfo">
    select xfmc as corpName, sum(hjje) as saleTotal
    from pa_head
    where
     DATE_SUB(CURDATE(), INTERVAL 1 YEAR) &lt;= date(kprq) and isvoid = false
    group by xfmc
  </select>
  <select id="getSellTotal" resultType="java.lang.Long">
    select sum(hjje) as saleTotal
    from pa_head
    where
     xfmc = #{cropName} and DATE_SUB(CURDATE(), INTERVAL 1 YEAR) &lt;= date(kprq) and isvoid = false
  </select>

  <select id="topConsumer" resultType="com.yiban.erp.entitiesPA.topConsumer">
    select sum(hjje) hj,gfmc
    from pa_head
    where
    xfmc =#{xfmc} and DATE_SUB(CURDATE(), INTERVAL 1 YEAR) &lt;= date(kprq) and isvoid = false
    group by gfmc
    order by hj desc
    limit 0,3
  </select>
  <select id="selectComp" resultType="java.lang.Integer">
    select count(*)
    from company_info
    where corp_name = #{corpName}
  </select>

